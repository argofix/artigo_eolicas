cres_eol <- read.csv2("dados/pot_anos.csv", h=T, sep=";")
cres_eol <- rename(cres_eol, ano = ï..ano) %>%
filter(ano < 2021)
cres_eol.melt <- reshape2::melt(cres_eol, id = "ano")
P1 <-  ggplot(data = cres_eol.melt, aes(x = ano, y = value, fill = variable))
P1 <-  P1 + geom_col(position = "dodge")
P1 <-  P1 + theme_classic()
P1 <- P1 + labs(title = "Evolução da energia eólica no Brasil",
subtitle = "2005 a 2020",
fill = "ano",
x = "Ano",
y = "Potência GW")
P1
